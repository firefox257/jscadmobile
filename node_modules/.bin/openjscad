#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/bin/nodejs" ]; then
  "$basedir//usr/bin/nodejs"  "$basedir/../jscad/openjscad" "$@"
  ret=$?
else 
  /usr/bin/nodejs  "$basedir/../jscad/openjscad" "$@"
  ret=$?
fi
exit $ret
